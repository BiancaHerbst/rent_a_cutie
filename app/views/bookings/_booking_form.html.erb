
<div class="container" data-unavailable="<%= @cutie.unavailable_dates.to_json %>" >
  <div class="form-wrapper " style="height: 200px; width: 300px; margin: 0 auto;">
    <%= simple_form_for @booking do |f| %>
      <%= f.input :start_date, as: :string, required: false, input_html: {id: "range_start"} %>
      <%#= f.input :start_date, as: :string, label: "Dates", placeholder: "From", wrapper_html: { class: "inline_field_wrapper" } %>
      <%= f.input :end_date, as: :string, required: false, input_html: {id: "range_end"} %>
      <%#= f.input :end_date, as: :string, label: '&nbsp;'.html_safe, placeholder: "To", disabled: true, wrapper_html: { class: "inline_field_wrapper" } %>
      <%= f.input :cutie_id, as: :hidden, input_html: { value: @cutie.id } %>
      <%= f.submit "Book Me", class: "btn btn-primary"  %>
       <%#= f.submit "book", class: "btn btn-primary btn-lg btn-block btn-booking" %>
      <%# raise %>
    <% end %>
  </div>
</div>
