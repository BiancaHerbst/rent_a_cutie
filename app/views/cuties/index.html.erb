<div class="flexbox">

<div class="container">
  <div class="row">
    <% @cuties.each do |c| %>
        <div class="col-sm-6 col-lg-4 col-xs-12">
            <%= render 'shared/card',cutie: c %>
         </div>
      <% end %>
    </div>
</div>

<div
  id="map"
  style="width: 60%;
  height: 600px;"
  data-markers="<%= @markers.to_json %>"
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY']%>"
></div>
</div>
